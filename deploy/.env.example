# Taler RPC Configuration
# Copy this file to .env and customize the values

# RPC username for authentication
TALER_RPCUSER=rpcuser

# RPC password - CHANGE THIS to a strong password
TALER_RPCPASSWORD=changeme

# RPC allowed IP addresses (0.0.0.0/0 allows all, restrict for security)
TALER_RPCALLOWIP=0.0.0.0/0
